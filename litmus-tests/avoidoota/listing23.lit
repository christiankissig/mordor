x := 0; y := 0; z := 0
{
  r1a := x; r1a := !!r1a
  r1b := x; r1b := !!r1b
  if (r1a != r1b) {
    z := 1
    r2 := y; r2 := !!r2
    r1 := r2;
  } else {
    r1 := r1b;
    r2 := y; r2 := !!r2
    if (r1 = r2) { z := 1 }
    else { z := 0 }
  }
}|||{{
  r3 := z
  y := r3
}|||{
  x := 1
}}
%% allow (r1=1 && r2=1) []